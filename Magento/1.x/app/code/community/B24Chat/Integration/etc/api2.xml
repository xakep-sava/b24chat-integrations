                    <description>Description</description>
                    <meta_keywords>Meta keywords</meta_keywords>
                    <meta_description>Meta description</meta_description>
                    <custom_layout_update>Custom layout update</custom_layout_update>
                    <available_sort_by>Available sort by</available_sort_by>
                    <upsale_image_link>Upsale image link</upsale_image_link>
                    <landing_page>Landing page</landing_page>
                    <is_anchor>Is anchor</is_anchor>
                    <custom_use_parent_settings>Custom use parent settings</custom_use_parent_settings>
