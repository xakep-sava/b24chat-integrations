                <group>b24chat_integration</group>
                <model>b24chat_integration/api2_websites</model>
                <title>Websites</title>
                <sort_order>10</sort_order>
                <versions>1</versions>
                <privileges>
                    <admin>
                        <retrieve>1</retrieve>
                    </admin>
                </privileges>
                <attributes translate="website_id code name sort_order default_group_id is_default"
                            module="b24chat_integration">
                    <website_id>Website ID</website_id>
                    <code>Code</code>
                    <name>Name</name>
                    <sort_order>Sort order</sort_order>
                    <default_group_id>Default group id</default_group_id>
                    <is_default>Is default</is_default>
                </attributes>
                <routes>
                    <route>
                        <route>/b24chat/websites</route>
                        <action_type>collection</action_type>
                    </route>
                </routes>
            </b24chat_integration_websites>
